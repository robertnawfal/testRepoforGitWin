Jenkinsfile (Declarative Pipeline)
pipeline {
    agent any
    stages {
        stage ('Compile Stage'){
            steps {
                bat 'java --version'
            }
        }
        stage ('Testing Stage'){
            steps {
                bat 'java --version'
            }
        }
        stage ('Deploy'){
            steps {
                bat 'java --version'
            }
        }
    }
}
